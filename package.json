{
  "name": "charge-sdk",
  "version": "0.5.2",
  "description": "Build, run and test tools for React projects written in TypeScript.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "babel-node --extensions \".ts\" ./src/cli.ts test",
    "build": "tsc --declaration",
    "release": "babel-node --extensions \".ts\" ./src/release.ts",
    "prepare": "npm run build"
  },
  "engines": {
    "node": "11.11.0",
    "npm": "6.9.0"
  },
  "bin": {
    "charge-sdk": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istvan-antal/charge-sdk.git"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Istv√°n Antal <i@istvan.xyz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/istvan-antal/charge-sdk/issues"
  },
  "homepage": "https://github.com/istvan-antal/charge-sdk#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-transform-react-jsx",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/autoprefixer": "^9.4.0",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^6.0.0",
    "@types/jest": "^24.0.11",
    "@types/jest-diff": "^20.0.1",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^11.12.0",
    "@types/postcss-nested": "^4.1.0",
    "@types/react-dev-utils": "^7.0.0",
    "@types/webpack": "^4.4.26",
    "@types/webpack-dev-server": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "autoprefixer": "^9.5.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.2.2",
    "jest": "^24.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-initial": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-math": "0.0.10",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "react-dev-utils": "^8.0.0",
    "read-cache": "^1.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.0.0",
    "typescript": "^3.3.4000",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.2.1"
  }
}
