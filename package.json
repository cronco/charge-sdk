{
  "name": "charge-sdk",
  "version": "0.4.4",
  "description": "Build, run and test tools for React projects written in TypeScript.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "babel-node --extensions \".ts\" ./src/cli.ts test",
    "build": "tsc --declaration",
    "release": "babel-node --extensions \".ts\" ./src/release.ts",
    "prepare": "npm run build"
  },
  "engines": {
    "node": "11.10.0",
    "npm": "6.8.0"
  },
  "bin": {
    "charge-sdk": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istvan-antal/charge-sdk.git"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Istv√°n Antal <i@istvan.xyz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/istvan-antal/charge-sdk/issues"
  },
  "homepage": "https://github.com/istvan-antal/charge-sdk#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-export-default-from",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/inquirer": "0.0.43",
    "@types/jest": "^24.0.6",
    "@types/jest-diff": "^20.0.1",
    "@types/node": "^11.9.4",
    "autoprefixer": "^9.4.8",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "cpp-wasm-loader": "^0.9.0",
    "css-loader": "^1.0.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.2.2",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-initial": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-math": "0.0.10",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.1",
    "postcss-simple-vars": "^5.0.2",
    "react-dev-utils": "^7.0.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.5",
    "webpack-dev-server": "^3.2.0"
  }
}
