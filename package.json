{
  "name": "charge-sdk",
  "version": "1.0.5",
  "description": "Build, run and test tools for React projects written in TypeScript.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "tsc --noEmit && eslint --ext ts,tsx --quiet ./src && bash -c \"babel-node --extensions '.ts' ./src/cli.ts test\"",
    "build": "bash -c \"tsc --declaration\"",
    "release": "bash -c \"babel-node --extensions '.ts' ./src/release.ts\"",
    "prepare": "npm run build"
  },
  "engines": {
    "node": "12.13.0",
    "npm": "6.12.0"
  },
  "bin": {
    "charge-sdk": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istvan-antal/charge-sdk.git"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Istv√°n Antal <i@istvan.xyz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/istvan-antal/charge-sdk/issues"
  },
  "homepage": "https://github.com/istvan-antal/charge-sdk#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-transform-react-jsx",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.19",
    "@types/jest-diff": "^24.3.0",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/node": "^12.12.15",
    "@types/postcss-nested": "^4.1.0",
    "@types/react-dev-utils": "^9.0.1",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "chalk": "^3.0.0",
    "css-loader": "^3.2.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss": "^7.0.24",
    "postcss-import": "^12.0.1",
    "postcss-initial": "^3.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-math": "0.0.10",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.1",
    "postcss-simple-vars": "^5.0.2",
    "react-dev-utils": "^10.0.0",
    "read-cache": "^1.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.8.2"
  }
}
